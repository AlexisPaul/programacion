#include "stdafx.h"
#include "stdio.h"
#include "conio.h"
#include "math.h"

void main()
{
	printf("\t CLAVE DEL SOFTWARE\n");
	printf("\t ================== \n");
	int clave, con;
	con = 0;
	do
	{
		printf("INGRESE CLAVE(intento: %d) : ",con+1);
		scanf_s("%d",&clave);
		con++;

	}while((clave != 78509594) && (con<3));
	if(clave==78509594)
		printf("CLAVE ACEPTADA");
	else
		printf("CLAVE ERRONEA, BLOQUEO DE SISTEMA");

	_getch();
}
