/* PROMEDIO DE 5 NUMEROS */
/*************************/ 

#include "stdafx.h"
#include "stdio.h"
#include "conio.h"


void main()
{ 
	float A,B,C,D,E,PROMEDIO;

	printf("\t	Calculo DEL PROMEDIO DE 5 NUMEROS\n");
	printf("\t	==================================\n");
	printf("INGRESE VALORES = \n");
	scanf("%f",&A);
	scanf("%f",&B);
	scanf("%f",&C);
	scanf("%f",&D);
	scanf("%f",&E);
	
	PROMEDIO=(A+B+C+D+E)/5;//DIVIDO 5 PORQUE ES UN PROGRAMA ELABORADO PARA 5 NUMEROS PROMEDIO

		printf("\nEL PROMEDIO ES DE: %f",PROMEDIO);
	
	getch();
}
